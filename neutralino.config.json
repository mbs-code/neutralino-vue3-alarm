{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/client/dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "extensions.*"
  ],
  "modes": {
    "window": {
      "title": "neutralino-vue3-alarm",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/client/dist/favicon.ico"
    }
  },
  "cli": {
    "binaryName": "neutralino-vue3-alarm",
    "resourcesPath": "/client/dist/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/client/public/neutralino.js",
    "frontendLibrary": {
      "patchFile": "/client/dist/index.html",
      "devUrl": "http://localhost:8080"
    },
    "binaryVersion": "4.2.0",
    "clientVersion": "3.1.0"
  }
}
